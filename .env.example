# RPC endpoint for contract deployments and bots
RPC=https://mainnet.infura.io/v3/YOUR_KEY

# Private key used to sign transactions
PRIV=0xYOUR_PRIVATE_KEY

# Address of the parimutuel betting pool contract
BSP=0xBSP_CONTRACT_AFTER_DEPLOY

# Address of the options desk contract
BBOD=0xOPTION_DESK_ADDRESS

# Address of the blob fee oracle contract
ORACLE=0xORACLE_ADDRESS

# Redis connection string for ephemeral state
REDIS=redis://localhost:6379

# Beacon node endpoint for blob fee data
BEACON=http://localhost:5052/eth/v1/debug/beacon/blob_fee

# (optional) WebSocket bridge port
WS_PORT=6380

# (optional) Prometheus metrics port
METRICS_PORT=9464
